{:paths ["src" "resources"]

 :deps
 {blaze/anomaly
  {:local/root "../anomaly"}

  cheshire/cheshire
  {:mvn/version "5.10.0"}

  com.github.ben-manes.caffeine/caffeine
  {:mvn/version "2.8.6"}

  com.google.guava/guava
  {:mvn/version "30.0-jre"}

  com.fasterxml.jackson.core/jackson-core
  {:mvn/version "2.11.3"}

  com.fasterxml.jackson.dataformat/jackson-dataformat-cbor
  {:mvn/version "2.11.3"}

  com.fasterxml.jackson.dataformat/jackson-dataformat-smile
  {:mvn/version "2.11.3"}

  com.taoensso/timbre
  {:mvn/version "4.10.0"}

  funcool/cuerdas
  {:mvn/version "2.2.1"}

  org.clojure/alpha.spec
  {:git/url "https://github.com/clojure/spec-alpha2.git"
   :sha "b67a00e5de4b5cde3bc9d7ef4f7c08f77c829ddd"}

  org.clojure/data.xml
  {:mvn/version "0.2.0-alpha6"}}

 :aliases
 {:test
  {:extra-paths ["test"]

   :extra-deps
   {criterium/criterium
    {:mvn/version "0.4.6"}

    lambdaisland/kaocha
    {:mvn/version "1.0.700"}

    org.clojars.akiel/iota
    {:mvn/version "0.1"}

    org.clojure/test.check
    {:mvn/version "1.1.0"}

    org.openjdk.jol/jol-core
    {:mvn/version "0.13"}}

   :main-opts
   ["-m" "kaocha.runner"]}

  :test-coverage
  {:extra-paths ["test"]

   :extra-deps
   {org.clojars.akiel/cloverage
    {:mvn/version "1.2.2-SNAPSHOT"}

    org.clojars.akiel/iota
    {:mvn/version "0.1"}

    org.clojure/test.check
    {:mvn/version "1.1.0"}

    org.openjdk.jol/jol-core
    {:mvn/version "0.13"}}

   :main-opts ["-m" "cloverage.coverage" "--codecov" "-p" "src" "-s" "test"]}}}
